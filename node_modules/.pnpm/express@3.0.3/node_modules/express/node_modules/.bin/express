#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/coding-practices/coding-practice-1h/backend/node_modules/.pnpm/express@3.0.3/node_modules/express/bin/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-1h/backend/node_modules/.pnpm/express@3.0.3/node_modules/express/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-1h/backend/node_modules/.pnpm/express@3.0.3/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-1h/backend/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-1h/backend/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-1h/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/coding-practices/coding-practice-1h/backend/node_modules/.pnpm/express@3.0.3/node_modules/express/bin/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-1h/backend/node_modules/.pnpm/express@3.0.3/node_modules/express/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-1h/backend/node_modules/.pnpm/express@3.0.3/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-1h/backend/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-1h/backend/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-1h/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/express" "$@"
else
  exec node  "$basedir/../../bin/express" "$@"
fi
